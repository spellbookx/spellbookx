{
  "name": "spellbookx",
  "version": "0.10.1",
  "private": false,
  "description": "CLI tool for the Spellbookx ecosystem to setup linting, formatting, and commit standards.",
  "keywords": [
    "cli",
    "spellbookx",
    "lint",
    "prettier",
    "eslint",
    "commitlint",
    "cspell"
  ],
  "homepage": "https://github.com/spellbookx/spellbookx#readme",
  "bugs": {
    "url": "https://github.com/spellbookx/spellbookx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spellbookx/spellbookx.git",
    "directory": "packages/spellbookx"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "sbx": "./dist/index.js",
    "spellbookx": "./dist/index.js"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "rolldown -c && tsc",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "degit": "^2.8.4",
    "execa": "^9.6.1",
    "inquirer": "^13.3.0"
  },
  "devDependencies": {
    "@dcdavidev/tsconfig": "^0.1.0",
    "@types/degit": "^2.8.6",
    "@types/node": "^25.3.0",
    "esmock": "^2.7.3",
    "rolldown": "1.0.0-rc.5",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
