{
  "name": "spellbookx",
  "version": "0.1.0",
  "private": false,
  "description": "CLI tool for the Spellbookx ecosystem to setup linting, formatting, and commit standards.",
  "keywords": [
    "cli",
    "spellbookx",
    "lint",
    "prettier",
    "eslint",
    "commitlint",
    "cspell"
  ],
  "homepage": "https://github.com/spellbookx/spellbookx#readme",
  "bugs": {
    "url": "https://github.com/spellbookx/spellbookx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spellbookx/spellbookx.git",
    "directory": "packages/spellbookx"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
