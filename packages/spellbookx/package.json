    "execa": "^9.5.2",
    "inquirer": "^12.4.2"
  },
  "devDependencies": {
    "@dcdavidev/tsconfig": "^0.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.0.10",
    "rolldown": "1.0.0-rc.1",
    "typescript": "5.9.2"
  },
  "publishConfig": {
