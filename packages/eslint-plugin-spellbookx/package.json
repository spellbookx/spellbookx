{
  "name": "eslint-plugin-spellbookx",
  "version": "0.7.2",
  "private": false,
  "description": "A comprehensive ESLint plugin with shareable configs for JS/TS, React, JSON, and more.",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "lint",
    "prettier",
    "spellbookx",
    "typescript"
  ],
  "homepage": "https://github.com/spellbookx/spellbookx#readme",
  "bugs": {
    "url": "https://github.com/spellbookx/spellbookx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spellbookx/spellbookx.git",
    "directory": "packages/eslint-plugin-spellbookx"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rolldown -c && tsc"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^9.6.2",
    "@eslint-react/eslint-plugin": "^2.7.4",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.4.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node-dependencies": "^1.3.0",
    "eslint-plugin-package-json": "^0.88.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vuoto": "^0.1.1",
    "globals": "^17.1.0",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript-eslint": "^8.54.0"
  },
  "devDependencies": {
    "@spellbookx/typescript-config": "workspace:*",
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "rolldown": "1.0.0-rc.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "eslint": "^9.39.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
