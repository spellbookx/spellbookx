{
  "name": "@spellbookx/eslint-plugin",
  "version": "0.1.4",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spellbookx/spellbookx",
    "directory": "libs/cspell-config"
  },
  "bugs": {
    "url": "https://github.com/spellbookx/spellbookx/issues"
  },
  "homepage": "https://github.com/spellbookx/spellbookx#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "tags": [
      "type:config-plugin"
    ],
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/eslint-plugin/dist",
          "main": "libs/eslint-plugin/src/index.ts",
          "tsConfig": "libs/eslint-plugin/tsconfig.lib.json",
          "format": [
            "esm"
          ],
          "declarationRootDir": "libs/eslint-plugin/src"
        }
      }
    }
  },
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-plugin-eslint-plugin": "^6.5.0",
    "eslint-plugin-n": "^17.21.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1"
  }
}
